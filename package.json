    "@reduxjs/toolkit": "^1.8.1",
    "@types/react-native-vector-icons": "^6.4.10",
    "axios": "^0.27.2",
    "dayjs": "^1.11.3",
    "detox": "^19.7.1",
    "i18next": "^21.6.16",
    "jest-circus": "27.5.1",
    "lodash": "^4.17.21",
    "patch-package": "^6.4.7",
    "prop-types": "^15.8.1",
    "qs": "^6.10.5",
    "react": "17.0.2",
    "react-i18next": "^11.16.7",
    "react-native": "0.67.4",
    "react-native-date-picker": "^4.2.2",
